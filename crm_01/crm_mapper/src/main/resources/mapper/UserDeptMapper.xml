<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kaishengit.crm.mapper.UserDeptMapper">
    <insert id="insert">
        INSERT into t_userDept(user_id, dept_id) values(#{userId},#{deptId})
    </insert>
    <delete id="delByUserId">
        DELETE FROM t_userdept WHERE user_id = ${userId}
    </delete>
</mapper>